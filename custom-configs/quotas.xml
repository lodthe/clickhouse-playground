<!-- Refer to the documentation for more detail: https://clickhouse.com/docs/en/operations/quotas/-->

<yandex>
    <quotas>
        <default>
            <interval>
                <duration>3600</duration>

                <queries>50</queries>
                <errors>50</errors>
                <result_rows>10000</result_rows>
                <read_rows>50000</read_rows>
                <execution_time>100</execution_time>
            </interval>
        </default>
    </quotas>
</yandex>
